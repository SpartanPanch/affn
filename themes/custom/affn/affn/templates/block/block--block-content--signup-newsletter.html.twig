<div class="join-nework signup-newsletter bg-red-gradient white-text pt-px-60 pb-px-60">
		<div class="container">
			<div class="row align-items-center">
				<div class="col-lg-6">
					<div class="signup-content">
						{% if content.body %}					
							{{ content.body }} 
						{% endif %}      
					</div>
				</div>
				<div class="col-lg-6">
					<div class="signup-newsform-wrap">					
						{{ title_prefix }}
						{% if label %}
							<h3 class="ft-26">{{ label|striptags }}</h3>
						{% endif %}
						{{ title_suffix }}
						<div class="signup-newsform c-form">
								  {# {{ drupal_entity('webform', 'subscribe_form') }}  	 #}
								   {{ drupal_region('subscribe') }}
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>